{
  "name": "eslint-plugin-miniprogram",
  "keywords": [
    "eslint",
    "plugin",
    "mini-program",
    "mini-app"
  ],
  "engines": {
    "node": ">= 8.0"
  },
  "repository": "git@github.com:airbnb/eslint-plugin-miniprogram.git",
